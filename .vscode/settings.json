{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.stylelint": "explicit",
    "source.addMissingImports": "explicit"
  },
  "cSpell.words": [
    "autodocs",
    "commitlint",
    "commitmsg",
    "esnext",
    "hygen",
    "markuplint",
    "nextjs",
    "opengift",
    "precommit",
    "tailwindcss"
  ]
}
