{
  "name": "opengift-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:unit": "jest  --passWithNoTests",
    "new:ui": "hygen generator ui",
    "new:model": "hygen generator model",
    "new:page": "hygen generator page",
    "prepare": "husky install",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "fmt": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss}'",
    "check:lint": "eslint 'src/**/*.{ts,tsx}' --report-unused-disable-directives --max-warnings 0",
    "check:cspell": "cspell -c ./cspell.config.json \"./src/**/*\" --unique --no-progress --no-summary",
    "check:markuplint": "markuplint -p \"./src/**/*.tsx\"",
    "check": "run-p check:* test:*",
    "ci:test-group": "run-p test:*",
    "ci:check": "run-p check:*",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "api:build": "aspida"
  },
  "engines": {
    "npm": "pnpm please",
    "yarn": "pnpm please",
    "bun": "pnpm please"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss}": [
      "pnpm check:lint --fix",
      "pnpm fmt"
    ]
  },
  "dependencies": {
    "@aspida/axios": "^1.14.0",
    "@chakra-ui/react": "^2.8.2",
    "@egoist/tailwindcss-icons": "^1.4.0",
    "@tanstack/react-table": "^8.10.7",
    "@types/lodash": "^4.14.202",
    "@types/react-datepicker": "^4.19.0",
    "aspida": "^1.14.0",
    "autoprefixer": "10.4.16",
    "axios": "^1.6.1",
    "class-variance-authority": "^0.7.0",
    "date-fns": "^2.30.0",
    "firebase": "^10.6.0",
    "lodash": "^4.17.21",
    "next": "13.5.6",
    "postcss": "8.4.32",
    "react": "18.2.0",
    "react-datepicker": "^4.20.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.4.3",
    "@markuplint/jsx-parser": "^3.11.0",
    "@markuplint/react-spec": "^3.13.0",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.2",
    "@storybook/addon-links": "^7.5.2",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.3",
    "@storybook/nextjs": "^7.6.2",
    "@storybook/react": "^7.5.2",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@tsconfig/strictest": "^2.0.1",
    "@types/jest": "^29.5.10",
    "@types/node": "20.10.4",
    "@types/react": "18.2.39",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.9.1",
    "cspell": "^7.3.8",
    "eslint": "8.55.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "hygen": "^6.2.11",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "lint-staged": "^15.0.2",
    "markuplint": "^3.15.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.1",
    "storybook": "^7.5.1",
    "ts-jest": "^29.1.1",
    "typescript": "5.3.2"
  },
  "volta": {
    "node": "20.10.0",
    "npm": "10.1.0",
    "pnpm": "8.7.4",
    "yarn": "3.6.3"
  }
}
